{
  "name": "linear-buildr",
  "version": "1.0.0",
  "description": "linear-buildr",
  "author": "liquefy",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "postinstall": "npx typechain --target ethers-v5 --out-dir 'assets/linearLibrary/linearJs/lib/contracts'  'assets/linearLibrary/linearJs/lib/abis/*.json'",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "ci-generate": "./scripts/export-env.sh && yarn generate",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@types/crypto-js": "^3.1.47",
    "@types/lodash": "^4.14.175",
    "@types/lz-string": "^1.3.34",
    "@types/node": "12.12.54",
    "@walletconnect/web3-provider": "^1.4.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "clipboard": "^2.0.6",
    "cross-env": "^5.2.0",
    "date-fns": "^2.15.0",
    "echarts": "^4.8.0",
    "ethers": "^5.4.7",
    "graph-results-pager": "~1.0.0",
    "graphql": "14.5.8",
    "husky": "^7.0.2",
    "jquery": "^3.4.1",
    "koa": "^2.6.2",
    "lint-staged": "^11.2.3",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "numbro": "^2.3.1",
    "nuxt": "^2.0.0",
    "prettier": "^2.4.1",
    "pubsub-js": "^1.9.2",
    "qrcode": "^1.4.4",
    "sass-loader": "^8.0.2",
    "secure-ls": "^1.2.6",
    "ts-loader": "^8.2.0",
    "typechain": "^5.2.0",
    "typescript": "^4.4.3",
    "view-design": "^4.2.0-rc.1",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-i18n": "^8.18.2",
    "vuex-persistedstate": "^3.0.1"
  },
  "browserslist": [
    "last 3 version",
    "> 1%",
    "iOS >= 8",
    "Firefox >= 20",
    "Android > 4.4"
  ]
